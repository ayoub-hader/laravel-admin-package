{
    "name": "neoxia/laravel-admin-package",
    "type": "library",
    "description": "sockle neoxia pour laravel avec BO",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravelcollective/html": "~6.0.3",
        "almasaeed2010/AdminLTE": "~3.0.1",
        "spatie/laravel-menu": "^3.4",
        "spatie/laravel-permission": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "LaravelAdminPackage\\": "src/",
            "LaravelAdminPackage\\App\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAdminPackage\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
